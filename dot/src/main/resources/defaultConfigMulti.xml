<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://openprovenance.org/model/opmPrinterConfig">

  <graphName>OPMGraph</graphName>


  <processes displayValue="true">
    <process value="http://process.org/add1ToAll" display="add1ToAll"/>
    <process value="http://process.org/split"     display="split"/>
    <process value="http://process.org/plus1"     display="+1"/>
    <process value="http://process.org/cons"      display="cons"/>
    <process value="http://process.org/fry"       display="fry"/>
    <process value="http://process.org/bake"      display="bake"/>
    <process value="http://process.org/badBake"   display="badBake"/>
  </processes>

  <artifacts displayValue="true">
    <artifact value="2 eggs"       display="two eggs"/>
  </artifacts>

  <agents displayValue="true">
  </agents>


  <edges default="filled">
    <edge type="org.openprovenance.model.Used"            style="filled"/>
    <edge type="org.openprovenance.model.WasGeneratedBy"  style="filled"/>
    <edge type="org.openprovenance.model.WasControlledBy" style="filled"/>
    <edge type="org.openprovenance.model.WasDerivedFrom"  style="filled"/>
    <edge type="org.openprovenance.model.WasTriggeredBy"  style="filled"/>

    <edge type="org.openprovenance.model.UsedStar"            style="dashed"/>
    <edge type="org.openprovenance.model.WasGeneratedByStar"  style="dashed"/>
    <edge type="org.openprovenance.model.WasDerivedFromStar"  style="dashed"/>
    <edge type="org.openprovenance.model.WasTriggeredByStar"  style="dashed"/>
  </edges>


  <accounts defaultAccount="black" defaultColor="black">
    <account name="orange"      color="red"/>
  </accounts>


</configuration>  
